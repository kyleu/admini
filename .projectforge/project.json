{
  "key": "admini",
  "name": "Admini",
  "icon": "compass",
  "version": "0.6.2",
  "package": "admini.dev/admini",
  "port": 14000,
  "modules": [
    "android",
    "core",
    "database",
    "desktop",
    "filesystem",
    "ios",
    "marketing",
    "mysql",
    "notarize",
    "oauth",
    "postgres",
    "sandbox",
    "search",
    "sqlite",
    "sqlserver",
    "themecatalog",
    "types",
    "upgrade"
  ],
  "ignore": [
    "^data"
  ],
  "tags": [
    "kyleu"
  ],
  "info": {
    "org": "kyleu",
    "authorID": "kyleu",
    "authorName": "Kyle U",
    "authorEmail": "kyle@kyleu.com",
    "license": "MIT",
    "homepage": "https://admini.dev",
    "sourcecode": "https://github.com/kyleu/admini",
    "summary": "Use Admini to explore and manage your data as fast and easily as possible",
    "description": "Explore and manage your data as fast and easily as possible. Supports PostgreSQL, MySQL, and SQLite data sources, and provides a lightning-fast web interface",
    "bundle": "com.kyleu.admini",
    "signingIdentity": "${APPLE_SIGNING_IDENTITY}",
    "javaPackage": "dev.admini"
  },
  "theme": {
    "base": "#8eb6bd",
    "light": {
      "brd": "1px solid #cccccc",
      "ld": "none",
      "fg": "#000000",
      "fgm": "#2f3d3f",
      "bg": "#ffffff",
      "bgm": "#e9eeef",
      "lf": "#102326",
      "lvf": "#102326",
      "nf": "#2a2a2a",
      "nb": "#93aeb3",
      "mf": "#000000",
      "msf": "#000000",
      "mb": "#d3dee0",
      "msb": "#a8bec2",
      "mbd": "rgba(77, 77, 77, .7)",
      "ok": "#008000",
      "err": "#ff0000"
    },
    "dark": {
      "brd": "1px solid #444444",
      "ld": "none",
      "fg": "#dddddd",
      "fgm": "#94a4a7",
      "bg": "#121212",
      "bgm": "#0f1c1e",
      "lf": "#d3dee0",
      "lvf": "#93aeb3",
      "nf": "#f8f9fa",
      "nb": "#102a2e",
      "mf": "#eeeeee",
      "msf": "#dddddd",
      "mb": "#102326",
      "msb": "#526c71",
      "mbd": "rgba(33, 33, 33, .7)",
      "ok": "#008000",
      "err": "#ff0000"
    }
  },
  "build": {
    "desktop": true,
    "notarize": true,
    "signing": true,
    "android": true,
    "iOS": true,
    "x86": true,
    "windowsARM": true,
    "linuxARM": true,
    "linuxMIPS": true,
    "linuxOdd": true,
    "dragonfly": true,
    "illumos": true,
    "freeBSD": true,
    "netBSD": true,
    "openBSD": true,
    "solaris": true,
    "homebrew": true,
    "nfpms": true,
    "snapcraft": true
  }
}
